{
  "expo": {
    "newArchEnabled": true,
    "name": "activity_diet_tracker_app",
    "slug": "activity_diet_tracker_app",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "android": {
      "package": "com.anonymous.activity_diet_tracker_app",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "ios": {
      "bundleIdentifier": "ios",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to show restaurant results."
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}
